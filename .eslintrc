{
  "env": { "browser": true },
  "extends": "airbnb-base",
	"rules": {
		"no-underscore-dangle": "off",
    "no-console": "off",
    "no-use-before-define": ["warn", { "functions": false, "variables": true}],
    "consistent-return": "off",
    "indent": ["error", "tab"],
    "no-tabs": "off"
    },
	"settings": {
		"import/resolver": "webpack"
	}
}
